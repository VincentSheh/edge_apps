    "p_k = [[0,1], [1,2/3]]\n",
    "\n",
    "for i in range(2,15):\n",
    "    p_k.append([i, phi_1*p_k[i-1][1] + phi_2*p_k[i-2][1]])\n",
    "\n",
    "df = pd.DataFrame(p_k, columns = [\"k\", \"p_k\"])\n",
    "df\n"
